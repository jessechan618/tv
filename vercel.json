{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    },
    {
      "src": "logo.png",
      "use": "@vercel/static"
    },
    {
      "src": "backend-vercel/api/twitch-status.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/twitch-status",
      "dest": "backend-vercel/api/twitch-status.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}